#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int i = 4;
    double d = 4.0;
    char s[] = "HackerRank ";

    int a;
    scanf("%d", &a);
    double b;
    scanf("%lf", &b);
    getchar();
    char c[101];
    fgets(c, sizeof(c), stdin);

    printf("%d\n", i + a);
    printf("%.1lf\n", d + b);
    printf("%s", s);
    puts(c);

    return 0;
}
